			".setColor('${1:GREEN}')",
			".setAuthor(`${message.author.tag}`, message.author.displayAvatarURL())",
			".setFooter(config.botname)",
			".setTimestamp()",
			".setDescription(`${emojis.yes} string`)",
			"message.lineReply(embed)"
